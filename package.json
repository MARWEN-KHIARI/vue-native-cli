{
  "name": "vue-native-cli",
  "version": "0.1.1",
  "description": "Command-Line Interface for Vue Native App",
  "author": "Geekyants (https://geekyants.com/)",
  "license": "MIT",
  "homepage": "https://github.com/GeekyAnts/vue-native-cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/GeekyAnts/vue-native-cli.git"
  },
  "bugs": {
    "url": "https://github.com/GeekyAnts/vue-native-cli/issues"
  },
  "keywords": [
    "vue",
    "native",
    "cli"
  ],
  "main": "./lib/index.js",
  "bin": {
    "vue-native": "./lib/index.js"
  },
  "preferGlobal": true,
  "engines": {
    "node": "^4.5 || >=5.10"
  },
  "engineStrict": true,
  "files": [
    "src/**"
  ],
  "dependencies": {
    "chalk": "^3.0.0",
    "commander": "^4.0.0",
    "cross-spawn": "^7.0.1",
    "inquirer": "^7.0.0",
    "lodash": "^4.17.15",
    "ora": "^4.0.2",
    "rimraf": "^3.0.0",
    "semver-regex": "^3.1.0"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.1",
    "@types/inquirer": "^6.5.0",
    "@types/lodash": "^4.14.145",
    "@types/node": "^12.12.6",
    "@types/rimraf": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^3.0.9",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2",
    "ts-node": "^8.4.1",
    "tslib": "^1.10.0",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "clean": "cross-env rm -rf ./lib",
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc -p .",
    "lint": "eslint ./ --ext .js,.ts",
    "lint:fix": "eslint ./ --ext .js,.ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run clean && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix"
    }
  }
}
